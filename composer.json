
{
    "name": "localheinz/symfony-live-berlin-2019-doctrine-tutorial",
    "description": "An application used for Marco Pivettas workshop during SymfonyLive 2019 in Berlin.",
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "ext-pdo_sqlite": "*",
        "doctrine/dbal": "^2.7.1",
        "doctrine/orm": "^2.6.1",
        "doctrine/data-fixtures": "^1.3.0"
    },
    "autoload": {
        "psr-4": {
            "Authentication\\": "src/Authentication"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7.1.5",
        "behat/behat": "^3.4.3"
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\Integration\\": "test/Integration",
            "Test\\Specification\\": "test/Specification",
            "Test\\Unit\\": "test/Unit"
        }
    }
}
